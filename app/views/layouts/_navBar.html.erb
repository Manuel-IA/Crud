<header class="mdc-top-app-bar app-bar" id="app-bar">
  <div class="mdc-top-app-bar__row">
    <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start">
    <% if login_user_signed_in? %>
      <button class="material-icons mdc-top-app-bar__navigation-icon mdc-icon-button">menu</button>
    <%  end  %>
    <span class="mdc-top-app-bar__title"><h1>Clients<h1></span>
  </section>
  <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end">
      <span class="mdc-button__label mdc-top-app-bar__title link_custom">
        <% if login_user_signed_in? %>
          <%= link_to  "Sign out", destroy_login_user_session_path, method: :delete%>
        <% else %>
          <%= link_to  "Login", new_login_user_session_path%>
        <% end %>
      </span>
      <%# <span class="material-icons mdc-fab__icon">keyboard_arrow_down</span> %>
    </div>
  </section>
  </div>
</header>